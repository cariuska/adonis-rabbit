{
  "name": "adonis-rabbit",
  "type": "module",
  "description": "AdonisJS RabbitMQ provider",
  "version": "0.1.1",
  "author": "Jota JÃºnior <jjunior@protonmail.ch>",
  "types": "build/adonis-typings/index.d.ts",
  "main": "build/providers/RabbitProvider.js",
  "license": "MIT",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/templates",
    "build/src",
    "build/standalone.js",
    "build/standalone.d.ts"
  ],
  "scripts": {
    "copy:templates": "copyfiles \"templates/**/*.txt\" build",
    "build": "tsc",
    "postbuild": "npm run copy:templates"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.1.8",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@swc/core": "^1.6.7",
    "adonis-preset-ts": "^1.0.4",
    "copyfiles": "^2.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-adonis": "^1.0.15",
    "eslint-plugin-prettier": "^3.4.0",
    "np": "^7.5.0",
    "prettier": "^2.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/amqplib": "^0.10.4",
    "amqplib": "^0.10.4",
    "fast-safe-stringify": "^2.0.7"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.0"
  },
  "adonisjs": {
    "templates": {
      "config": {
        "src": "rabbit.txt",
        "dest": "rabbit"
      }
    },
    "env": {
      "RABBITMQ_HOSTNAME": "localhost",
      "RABBITMQ_USER": "",
      "RABBITMQ_PASSWORD": "",
      "RABBITMQ_PORT": "",
      "RABIITMQ_PROTOCOL": "ampq://"
    },
    "types": "adonis-rabbit",
    "providers": [
      "adonis-rabbit"
    ]
  },
  "np": {
    "message": "Releases v%s"
  },
  "repository": "https://github.com/jotaajunior/adonis-rabbit.git",
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",

  "extends": "@adonisjs/tsconfig/tsconfig.package.json",
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./build"
  }
}
